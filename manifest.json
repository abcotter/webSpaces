{
	"name": "Spaces",
	"description": "Build an Extension!",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"options_page": "options.html",
	"permissions": [
		"storage",
		"activeTab",
		"scripting",
		"tabs",
		"sessions"
	],
	"action": {
		"default_popup": "popup.html"
	}
}