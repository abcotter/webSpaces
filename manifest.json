{
	"name": "StateStore",
	"description": "Save your browsers state for later!",
	"version": "0.0.0.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "assets/icon.png",
		"48": "assets/icon.png",
		"128": "assets/icon.png"
	},
	"permissions": [
		"storage",
		"tabs"
	],
	"action": {
		"default_popup": "popup.html"
	}
}